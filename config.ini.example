[Cloudflare]
api_url = https://healthcheckwatch.your-subdomain.workers.dev
api_token = YOUR_SECRET_TOKEN

[Settings]
# Set to 'yes' to log alerts without sending emails (e.g., during planned downtime)
squelch = no

[SMTP]
host = mail.host.net
port = 465
user = user@domain.net
pass = your_raw_password!
# Set to 'yes' for port 465 (Implicit SSL), 'no' for port 587 (STARTTLS)
use_ssl = yes
